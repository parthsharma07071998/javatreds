{
"name" : "payfile",
"tableName" : "PaymentFiles",
"columnPrefix" : "PF",
"desc" : "PaymentFiles : The list of pay-in files generated or return files uploaded. It records the date of upload, the facilitator, total value and the record count, and the current status of the file transaction.",
"fields" : [
		{
			"name": "id",
			"fieldType":"PRIMARY",
			"dataType":"INTEGER",
			"notNull": true,
			"desc":"Internal unique identifier of the record."
		},
		{
			"name":"date",
			"label":"Date",
			"dataType":"DATE",
			"notNull": true,
			"desc":"The date on which the pay-in file generated of the return file uploaded."
		},
		{
			"name":"payfileType",
			"label":"Payment File Type",
			"dataType":"STRING",
			"maxLength": 1,
			"dataSetType": "STATIC",
			"dataSetValues": [{"text":"Interim Debit File", "value":"D"},{"text":"Full File", "value":"F"},{"text":"Interim Credit File", "value":"C"}],
			"desc":"The type of the file."
		},
		{
			"name":"fileType",
			"label":"File Type",
			"dataType":"STRING",
			"maxLength": 1,
			"notNull": true,
			"dataSetType": "ENUM",
			"dataSetValues": "com.xlx.treds.auction.bean.ObligationBean.TxnType",
			"desc":"Type of transaction debit/credit."
		},
		{
			"name":"facilitator",
			"label":"Facilitator",
			"dataType":"STRING",
			"maxLength": 50,
			"notNull": true,
			"dataSetType":"REFCODE",
			"dataSetValues":"FACILITATOR"
		},
		{
			"name":"fileName",
			"label":"File Name",
			"dataType":"STRING",
			"maxLength": 50,
			"notNull": true
		},
		{
			"name":"recordCount",
			"label":"No of Records",
			"dataType":"INTEGER",
			"notNull": true
		},
		{
			"name":"totalValue",
			"label":"Total Value",
			"dataType":"DECIMAL",
			"integerLength":10,
			"decimalLength":2,
			"notNull": true,
			"minValue": 0.01
		},
		{
			"name":"generatedByAuId",
			"label":"Generated By",
			"dataType":"INTEGER",
			"notNull": true,
			"desc":"Internal login-id information of the user generating/uploading the file."
		},
		{
			"name":"generatedTime",
			"label":"Generated Time",
			"dataType":"DATETIME",
			"notNull": true
		},
		{
			"name":"status",
			"label":"Status",
			"dataType":"STRING",
			"maxLength": 1,
			"notNull": true,
			"dataSetType": "STATIC",
			"dataSetValues": [{"text":"Generated", "value":"G"},{"text":"Return File Uploaded", "value":"R"}],
			"desc":"The current status of the file. Whether the file is generated or return file is uploaded after the generation."
		},
		{
			"name":"returnUploadedByAuId",
			"label":"Return file uploaded by",
			"dataType":"INTEGER",
			"desc":"Internal id of the logged-in-user uploading the return file."
		},
		{
			"name":"skipL1FileGeneration",
			"label":"Skip Leg 1 File Generation",
			"dataType":"STRING",
			"maxLength": 1,
			"dataSetType":"ENUM",
			"dataSetValues":"com.xlx.commonn.CommonAppConstants.Yes",
			"nonDatabase":true
		},
		{
			"name":"returnUploadedTime",
			"label":"Return upload time",
			"dataType":"DATETIME"
		},
		{
			"name": "recordVersion",
			"fieldType":"RECORDVERSION"
		}
	],
	"fieldGroups": {
		"insert":["date","fileType","facilitator","skipL1FileGeneration"]
		,"return": ["status","recordCount","totalValue","returnUploadedByAuId","returnUploadedTime","payfileType"]
    }
}